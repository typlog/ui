    "@types/node": "^24.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-typescript": "^14.1.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.0.0",
    "markdown-it": "^14.1.0",
    "postcss": "^8.4.30",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.0",
    "unplugin-icons": "^22.1.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.2.1",
