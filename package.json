{
  "name": "@typlog/ui",
  "version": "0.11.2",
  "license": "MIT",
  "type": "module",
  "description": "Themed components for Reka UI.",
  "author": "Hsiaoming Yang <me@lepture.com>",
  "repository": {
    "url": "https://github.com/typlog/ui"
  },
  "sideEffects": false,
  "module": "./dist/components.js",
  "types": "./dist/components.d.ts",
  "exports": {
    ".": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js"
    },
    "./addons": {
      "types": "./dist/addons.d.ts",
      "import": "./dist/addons.js"
    },
    "./base.css": "./dist/base.css",
    "./components.css": "./dist/components.css",
    "./addons.css": "./dist/addons.css",
    "./tailwind.css": "./tailwind/index.css",
    "./components/": "./dist/components/"
  },
  "scripts": {
    "start": "vite",
    "build": "npm run build:vue && npm run build:css",
    "build:vue": "vite build && node scripts/post-build.mjs",
    "build:css": "node scripts/build-css.mjs",
    "lint": "eslint src"
  },
  "files": [
    "./LICENSE",
    "./dist",
    "./tailwind"
  ],
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "reka-ui": "^2.0.0",
    "vue": "^3.5.0"
  },
  "devDependencies": {
    "@radix-ui/colors": "^3.0.0",
    "@tailwindcss/vite": "^4.0.6",
    "@types/node": "^24.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-typescript": "^14.1.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.0.0",
    "postcss": "^8.4.30",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.2.1",
    "vue-tsc": "^3.0.0"
  }
}
